{
	"name": "g14controlv2",
	"productName": "G14ControlV2",
	"version": "v0.1.28",
	"description": "Management Tools for the Zephyrus G14",
	"author": {
		"name": "Alex Redden",
		"url": "https://github.com/aredden"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/aredden/electron-g14control.git"
	},
	"homepage": "./",
	"main": "build/electron.js",
	"dependencies": {
		"@reduxjs/toolkit": "^1.5.0",
		"antd": "^4.7.0",
		"chalk": "^4.1.0",
		"chart.js": "^2.9.4",
		"chartjs-plugin-annotation": "^0.5.7",
		"chartjs-plugin-dragdata": "^1.1.3",
		"dotenv": "^8.2.0",
		"electron-devtools-installer": "^3.1.1",
		"electron-is-accelerator": "^0.2.0",
		"electron-is-dev": "^1.2.0",
		"electron-updater": "^4.3.5",
		"fkill": "^7.0.1",
		"forcefocus": "^1.1.0",
		"is-admin": "^3.0.0",
		"lodash": "^4.17.20",
		"node-hid": "^2.0.0-0",
		"node-powershell": "^4.0.0",
		"node-sass": "^4.14.1",
		"perfmon": "^0.2.0",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-markdown": "^5.0.3",
		"winston": "^3.3.3",
		"wmi-client": "^0.5.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"tsc": "tsc",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"electron-start": "electron ./build/electron.js",
		"preelectron-pack": "prebuild-install",
		"electron-build": "npm run build && npm run dist",
		"pack": "electron-builder --dir",
		"electron-pub": "electron-builder build --publish always",
		"dist": "electron-builder --publish always",
		"postinstall": "electron-builder install-app-deps",
		"build-electron": "call \"./build-all.bat\"",
		"build-github": "call \"./github-build.bat\""
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"build": {
		"appId": "g14control.main",
		"nsis": {
			"oneClick": false,
			"perMachine": true,
			"allowToChangeInstallationDirectory": false,
			"license": "LICENSE"
		},
		"win": {
			"target": "nsis",
			"publish": {
				"provider": "github",
				"private": false
			},
			"icon": "./electron/src/assets/icon.ico",
			"requestedExecutionLevel": "requireAdministrator",
			"extraResources": [
				{
					"from": "./electron/src/assets",
					"to": "extraResources",
					"filter": [
						"**/*"
					]
				},
				{
					"from": "./electron/src/Registry",
					"to": "extraResources",
					"filter": [
						"*.exe"
					]
				},
				{
					"from": "./electron/restartgpu",
					"to": "extraResources",
					"filter": [
						"**/*"
					]
				},
				{
					"from": "./electron/src/config",
					"to": "extraResources",
					"filter": [
						"*config.json"
					]
				},
				{
					"from": "./electron/screen-resolution",
					"to": "extraResources",
					"filter": [
						"**/*"
					]
				},
				{
					"from": "./electron/atrofac-cli",
					"to": "extraResources",
					"filter": [
						"**/*"
					]
				},
				{
					"from": "./electron/ryzenadj",
					"to": "extraResources",
					"filter": [
						"**/*"
					]
				}
			]
		}
	},
	"jest": {
		"moduleNameMapper": {
			"electron": "<rootDir>/src/tests/mock/electron.js"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"@types/antd": "^1.0.0",
		"@types/chart.js": "^2.9.27",
		"@types/chartjs-plugin-annotation": "^0.5.1",
		"@types/electron": "^1.6.10",
		"@types/jest": "^24.0.0",
		"@types/lodash": "^4.14.165",
		"@types/node": "^12.0.0",
		"@types/react": "^16.9.0",
		"@types/react-dom": "^16.9.0",
		"babel-cli": "^6.26.0",
		"electron": "^10.1.6",
		"electron-builder": "^22.9.1",
		"electron-rebuild": "^2.3.4",
		"react-scripts": "3.4.3",
		"typescript": "^4.1.2"
	}
}
